{
  "name": "Quaerere",
  "version": "0.1.0",
  "description": "A project developed under LGP, a course unit @ FEUP",
  "private": true,
  "main": "public/electron.js",
  "homepage": "./",
  "author": "Feeka",
  "dependencies": {
    "bootstrap": "^4.3.1",
    "concurrently": "^4.1.0",
    "cross-env": "^5.2.0",
    "dotenv": "^7.0.0",
    "electron-is-dev": "^1.0.1",
    "msal": "^0.2.4",
    "node-sass": "^4.11.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-quill": "^1.3.3",
    "react-router-dom": "^5.0.0",
    "react-scripts": "2.1.8",
    "reactstrap": "^8.0.0",
    "request": "^2.88.0",
    "request-promise": "^4.2.4",
    "wait-on": "^3.2.0"
  },
  "scripts": {
    "react-start": "react-scripts start",
    "react-build": "react-scripts build",
    "react-test": "react-scripts test",
    "react-eject": "react-scripts eject",
    "electron-start": "electron .",
    "electron-build": "electron-builder",
    "build": "yarn react-build && yarn electron-build",
    "start": "concurrently \"cross-env BROWSER=none yarn react-start\" \"wait-on http://localhost:3000 && electron .\""
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^5.8.1",
    "electron": "^4.1.4",
    "electron-builder": "^20.39.0",
    "typescript": "^3.4.1"
  },
  "build": {
    "appId": "com.example.quaerere",
    "mac": {
      "target": "dmg"
    },
    "win": {
      "target": "nsis"
    },
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  }
}
